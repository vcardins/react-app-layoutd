{
	"name": "react-app-layouts",
	"version": "1.1.0",
	"description": "",
	"main": "./dist/cjs/index.js",
	"module": "./dist/cjs/index.js",
	"types": "./dist/cjs/types/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vcardins/react-app-layouts.git"
	},
	"files": [
		"dist/**",
		"!dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/types/index.d.ts",
				"default": "./dist/esm/index.mjs"
			},
			"require": {
				"types": "./dist/cjs/types/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"scripts": {
		"clean": "rm -rf ./dist",
		"lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
		"prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
		"test": "jest --config jestconfig.json",
		"build": "npm run clean && npm run build:esm && npm run build:cjs",
		"build:esm": "tsc -p ./tsconfigs/tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
		"build:cjs": "tsc -p ./tsconfigs/tsconfig.cjs.json",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint"
	},
	"keywords": [
		"react",
		"layouts",
		"material design",
		"mui"
	],
	"author": "Victor Cardins (vcardins)",
	"license": "MIT",
	"devDependencies": {
		"@emotion/babel-plugin": "11.10.5",
		"@emotion/eslint-plugin": "^11.10.0",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.2.1",
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"@types/react-router": "^5.1.19",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest": "^29.2.2",
		"jest-canvas-mock": "^2.4.0",
		"jest-environment-jsdom": "^29.2.2",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"semantic-release": "^19.0.5",
		"ts-jest": "^29.0.3",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@mui/icons-material": "^5.10.9",
		"@mui/material": "^5.10.12",
		"deepmerge-ts": "^4.2.2",
		"react-router": "^6.4.3",
		"react-router-dom": "^6.4.3",
		"react-spring": "^9.5.5"
	},
	"peerDependencies": {
		"react": ">=17"
	},
	"alias": {
		"context": "./src/context",
		"types": "./src/types",
		"components": "./src/components"
	}
}
